<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="589.0" prefWidth="483.0" stylesheets="@GameTheme.css" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.BlackJackController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button id="btnBOTTOM" fx:id="btnBOTTOM" mnemonicParsing="false" onAction="#btnSitLeave_Click" text="Sit" translateX="-100.0">
               <padding>
                  <Insets bottom="75.0" top="100.0" />
               </padding>
            </Button>
            <Label id="lblBOTTOM" fx:id="lblBOTTOM" />
        </buttons>
      </ButtonBar>
   </bottom>
   <top>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button id="btnTOP" fx:id="btnTOP" mnemonicParsing="false" onAction="#btnSitLeave_Click" text="Sit" translateX="-100.0" />
            <Label id="lblTOP" fx:id="lblTOP" />
        </buttons>
      </ButtonBar>
   </top>
</BorderPane>
